{
  "root": true,
  "extends": [
    "airbnb",
    "airbnb-typescript"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": [
      "./tsconfig.json"
    ]
  },
  "rules": {
    "@typescript-eslint/comma-dangle": "off",
    "import/extensions": "off"
  },
  "plugins": [
    "@typescript-eslint"
  ]
}